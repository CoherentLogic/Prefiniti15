<script type="text/javascript"> 
function changeLanguage(language) {
  var host = 'http://'+this.location.host;
  var currentURL = this.location.toString();
  var currentPath = currentURL.substring(host.length);
  if (language == 'RU') host = 'http://www.diskinternals.ru';
  else host = 'http://www.diskinternals.com';

  _gaq.push(['_trackEvent','go','to_ru']);
  this.location = host + currentPath;
}
</script> 


